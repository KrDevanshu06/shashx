{
	"name": "shashx",
	"scopeName": "source.shashx",
	"patterns": [
	  {
		"match": "~>.*$",
		"name": "comment.line.shashx"
	  },
	  {
		"match": "\\b(pf|if|elf|el|flp|wlp|dlp)\\b",
		"name": "keyword.control.shashx"
	  },
	  {
		"match": "@[a-zA-Z_][a-zA-Z0-9_]*",
		"name": "variable.other.declaration.shashx"
	  },
	  {
		"match": "\\b[0-9]+\\b",
		"name": "constant.numeric.shashx"
	  },
	  {
		"match": "\"[^\"]*\"",
		"name": "string.quoted.double.shashx"
	  },
	  {
		"match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
		"name": "variable.other.shashx"
	  },
	  {
		"match": "[=+\\-*/<>!]+",
		"name": "keyword.operator.shashx"
	  }
	],
	"repository": {},
	"scopeName": "source.shashx"
  }
  